				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Initial_InjectionStorage_Capacity/   MAX(stored_CO2, initial_stored_CO2_for_the_consideration_of_cost_effects))^learning_rate_parameter</eqn>
				<units>dmnl</units>
			</aux>
