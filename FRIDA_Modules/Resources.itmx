				<units>MtCO2/Year</units>
			</aux>
			<aux name="CO2 emissions from oil" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>MtCO2/Year</units>
			</aux>
			<aux name="CO2 emissions from gas" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>MtCO2/Year</units>
			</aux>
			<aux name="CO2 emissions from coal" access="input">
				<dimensions>
