				</dimensions>
				<eqn>share_of_coal_emissions_stored*CO2_emissions_from_coal</eqn>
				<units>MtCO2/Year</units>
			</aux>
			<aux name="storing carbon from gas\nemissions">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
