			</aux>
			<flow name="Depreciation of\nBECCS">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>BECCS[Run]*depreciation_rate_of_BECCS_capital[Run]</eqn>
				<non_negative/>
				<units>MtCO2/Year</units>
			</flow>
			<stock name="BECCS" access="output" isee:autocreated="true">
				<doc>${Calibrated Output}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Initial_BECCS[Run]</eqn>
				<inflow>Installation_of_BECCS_Capacity</inflow>
				<outflow>Depreciation_of_BECCS</outflow>
				<non_negative/>
				<units>MtCO2</units>
			</stock>
			<aux name="Initial BECCS">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>0.0</eqn>
				<range min="50" max="150"/>
				<units>MtCO2</units>
			</aux>
			<flow name="Installation of\nBECCS Capacity">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Investment_in_BECCS)/cost_of_capturing_CO2_per_unit_with_BECCS</eqn>
				<non_negative/>
				<units>MtCO2/Year</units>
			</flow>
			<aux name="BECCS CO2 Captured" access="output" isee:autocreated="true">
				<doc>${Calibrated Output}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>BECCS*Efficiency_for_BECCS*UNIT_to_per_year</eqn>
				<units>MtCO2/Year</units>
			</aux>
			<aux name="Additional Investment in BECCS">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(storing_carbon_from_biofuel_emissions)*cost_of_capturing_CO2_per_unit_with_BECCS</eqn>
				<units>Mc$</units>
